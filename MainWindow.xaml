                                <ComboBox x:Name="Pen" Height="25" Width="75" HorizontalAlignment="Right" />
                                <TextBox x:Name="Velocity" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Range" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Radius" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Lifetime" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Anticipation" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="MaxFX" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Delay" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Burst" Height="25" Width="50" HorizontalAlignment="Right" />
                                <TextBox x:Name="Wait" Height="25" Width="50" HorizontalAlignment="Right" />
